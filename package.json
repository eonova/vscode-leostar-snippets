{
  "publisher": "leostar-tools",
  "name": "leostar-tools",
  "displayName": "leostar-tools",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "description": "leostar's vscode extension",
  "author": "LeoStar <hi@leostar.top>",
  "license": "MIT",
  "homepage": "https://github.com/ileostar/leostar-tools#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ileostar/leostar-tools"
  },
  "bugs": {
    "url": "https://github.com/ileostar/leostar-tools/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.83.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "leostar-tools.leostar",
        "title": "Leostar: 简短标题"
      },
      {
        "command": "leostar-tools.leostar2",
        "title": "Leostar: 插入代码片段"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@antfu/ni": "^0.21.9",
    "@types/node": "^20.9.0",
    "@types/vscode": "^1.84.1",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "esno": "^4.0.0",
    "pnpm": "^8.10.5",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
