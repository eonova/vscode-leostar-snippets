{
  "publisher": "leostar-tools",
  "name": "leostar-snippets",
  "displayName": "leostar-snippets",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.3.0",
  "description": "leostar's vscode snippets",
  "author": "LeoStar <hi@leostar.top>",
  "license": "MIT",
  "homepage": "https://github.com/ileostar/vscode-leostar-snippets#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ileostar/vscode-leostar-snippets"
  },
  "bugs": {
    "url": "https://github.com/ileostar/vscode-leostar-snippets/issues"
  },
  "keywords": [
    "Vue 3",
    "Snippets"
  ],
  "categories": [
    "Snippets"
  ],
  "icon": "res/icon.png",
  "engines": {
    "vscode": "^1.87.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      }
    ]
  },
  "scripts": {
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.3",
    "@antfu/ni": "^0.21.12",
    "@types/node": "^20.11.28",
    "@types/vscode": "^1.87.0",
    "@vscode/vsce": "^2.24.0",
    "bumpp": "^9.4.0",
    "esno": "^4.7.0",
    "pnpm": "^8.15.5"
  }
}
